{
  "name": "gh-pages-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-template-typescript": "1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1"
  },
  "homepage": "https://robzarel.github.io/gh-pages-demo",
  "scripts": {
    "start": "react-scripts start",
    "serve": "json-server --watch ./src/server/db/index.js --routes ./src/server/routes.json --port 3001",
    "save-json-api": "node ./src/srcipts/save-json-api.js",
    "build": "react-scripts build && npm run save-json-api",
    "predeploy": "rm -rf build && npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",,
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.2.0",
    "json-server": "^0.17.0",
    "node-fs": "^0.1.7"
  },
  "proxy": "http://localhost:3001"
}
